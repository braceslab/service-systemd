{
  "name": "my-service",
  "description": "an amazing service",
  "author": "John Smith",
  "cwd": "/node/my-service/",
  "user": "www-data",
  "group": "www-data",
  "env": {
    "VAR_NAME": "VAR_VALUE"
  },
  "app": "main.js",
  "app.args": "-debug",
  "pid": "/var/run/my-service.pid",
  "log": "/var/log/my-service/log",
  "error": "/var/log/my-service/error",
  "engine": "node",
  "engine.bin": "/usr/bin/node",
  "engine.args": "",
  "logrotate": true,
  "logrotate.rotate": "10",
  "logrotate.frequency": "daily"
}